export component EvalBar inherits Rectangle {
    in property <percent> b_eval: 50%;
    in property <percent> w_eval: 50%;

    height: 100%;
    width: 20px;

    Rectangle {
        x: 0;
        y: 0;
        height: b_eval;
        background: black;
    }
    Rectangle {
        y: parent.height - (parent.height * w_eval);
        height: w_eval;
        background: white;
    }
}
